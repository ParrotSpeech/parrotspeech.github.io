# Git LFS tracking for large model files
*.onnx filter=lfs diff=lfs merge=lfs -text

# Specific model files
web/public/models/onnx/model_q8f16.onnx filter=lfs diff=lfs merge=lfs -text
web/public/models/onnx/model_quantized.onnx filter=lfs diff=lfs merge=lfs -text

# Other common large file formats
*.bin filter=lfs diff=lfs merge=lfs -text
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.safetensors filter=lfs diff=lfs merge=lfs -text
*.wasm filter=lfs diff=lfs merge=lfs -text
